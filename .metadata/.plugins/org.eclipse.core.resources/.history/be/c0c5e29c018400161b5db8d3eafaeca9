package tp2;
import java.util.Scanner;
public class TP2 {
	
	public static void exercice1a() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir deux nombres");
		int val1 = sc.nextInt();
		int val2 = sc.nextInt();
		int valMax;
		if (val1 > val2) {
			valMax = val1;
		}
		else {
			valMax = val2;
		}
		System.out.println("La valeur maximum est " + valMax);
	}
	
	public static void exercice1b() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir trois nombres : ");
		int val1 = sc.nextInt();
		int val2 = sc.nextInt();
		int val3 = sc.nextInt();
		int valMax;
		if (val1 > val2) {
			valMax = val1;
			if (val3 > val1) {
				valMax = val3;
			}
		}
		else {
			valMax = val2;
			if (val3 > val2) {
				valMax = val3;
			}
		}
		System.out.println("La valeur maximale est : " + valMax);
	}

	public static void exercice1c() {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir quatre nombres : ");
		int val1 = sc.nextInt();
		int val2 = sc.nextInt();
		int val3 = sc.nextInt();
		int val4 = sc.nextInt();
		int valMax;
		if (val1 > val2) {
			// Si la valeur 1 est plus grande
			valMax = val1;
			if (val3 > val1) {
				// Si valeur 3 est plus grande
				valMax = val3;
				if (val4 > val3) {
					// Si valeur 4 est plus grande
					valMax = val4;
				}
			}
		}
		else {
			// Si valeur 2 est plus grande
			valMax = val2;
			if (val3 > val2) {
				// Si valeur 3 est plus grande
				valMax = val3;
				if (val4 > val3) {
					// Si valeur 4 est plus grande
					valMax = val4;
				}
			}
		}
		System.out.println("Valeur max : " + valMax);
	}

	public static void exercice1d() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir 2 nombres");
		int a = sc.nextInt();
		int b = sc.nextInt();
		if (a > b) {
			System.out.println(a + " " + b);
		}
		else {
			System.out.println(b + " " + a);
		}
	}
	
	public static void exercice1e() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir 3 nombres");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		int first;
		int middle;
		int last;
		if ((a > b) && (a > c)) {
			first = a;
			if (b > c) {
				middle = b;
				last = c;
			}
			else {
				middle = c;
				last = b;
			}
		}
		else if ((b > a) && (b > c)) {
			first = b;
			if (a > c) {
				middle = a;
				last = c;
			}
			else {
				middle = c;
				last = a;
			}
		}
		else {
			first = c;
			if (a > b) {
				middle = a;
				last = b;
			}
			else {
				middle = b;
				last = a;
			}
		}
		System.out.println(first + " " + middle + " " + last);
	}
	
	public static void exercice2a() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir 2 entiers");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int i = 0; // Soit la valeur pour iteration
		int res = 0; // Variable pour stocker le resultat
		int lim = b < 0 ? - b : b;
		
		for (i = 0; i < lim; i++) {
				res = res + a;
		}
		if (b < 0) { // Dans le cas ou b est negatif
			res = - res;
		}
		System.out.println(7 * -5);
		System.out.println(res);
	}
	
	public static void exercice2b() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir 2 entiers");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int i = 0; // Soit valeur pour iteration SEE exercice2a()
		int j = 0;
		int res = 0; // Variable pour stocker le resultat SEE exercice2a();
		int res2 = 0;
		int lim = a < 0 ? - a : a;
		int lim2 = b < 0 ? - b : b;
		for (i = 0; i < lim; i++) {
			res = res + a;
		}
		if (a < 0) {
			res = - res;
		}
		for (j= 0; j < lim2; j++) {
			res2 = res2 + res;
		}
		if (b < 0) {
			res2 = - res2;
		}
		System.out.println(res);
		// DEL pour test System.out.println(a*a);
		System.out.println(res2);
		// DEL pour test System.out.println((a*a)*b);
	}
	public static void exercice2c() { // TODO
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir 2 entiers");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int i = 0; // Valeur pour iteration
		int mod = 0; // Resultat du modulo
		int a2 = 0;
		for (i = 0; i < a; i++) {
			a2 = a - b;
			mod = a2 - i;
		}
		System.out.println(a%b);
		System.out.println(mod);
	}
	
	public static void exercice2d() {
		Scanner sc = new Scanner(System.in);
		System.out.println("Saisir deux entiers");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int i = 0; // Valeur pour iteration
		int lim = b;
		int res = a; // Variable pour stocker le resultat
		
		for (i = 0; i < lim; i++) {
			res = a - b;
			res = a - i;
		}
		System.out.println(6/3);
		System.out.println(res);
	}
	
	public static void main(String[] args) {
		// exercice1a();
		// exercice1b();
		// exercice1c();
		// exercice1d();
		// exercice1e();
		//exercice2a();
		// exercice2b();
		// TODO exercice2c();
		exercice2d();

	}

}
